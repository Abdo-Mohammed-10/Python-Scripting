    "import os\n",
    "def current_dir():\n",
    "    cwd=os.getcwd()\n",
    "    print(cwd)\n",
    "def file_path(filename):\n",
    "    path=os.path.abspath((filename)) \n",
    "    print(path)\n",
    "current_dir()\n",
    "filename=\"simple.txt\"\n",
    "file_path(filename)"
